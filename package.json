{
  "name": "contact-keeper",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "knex migrate:up --env test",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "posttest": "knex migrate:down --env test",
    "start": "npm run build:ts && fastify start -l info dist/src/app.js",
    "build:ts": "tsc",
    "dev": "cross-env NODE_ENV='development' tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js\"",
    "migrate:make": "knex migrate:make contact_keeper -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "fastify": "^3.0.0",
    "fastify-auth": "^1.1.0",
    "fastify-autoload": "^3.3.1",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cli": "^2.14.0",
    "fastify-cookie": "^5.4.0",
    "fastify-env": "^2.1.1",
    "fastify-jwt": "4.1.3",
    "fastify-plugin": "^3.0.0",
    "fastify-redis": "^4.3.3",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^4.13.0",
    "ioredis": "^4.28.2",
    "knex": "^0.95.15",
    "knex-tiny-logger": "^2.1.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ioredis": "^4.28.5",
    "@types/node": "^16.0.0",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^15.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
