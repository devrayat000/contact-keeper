{
  "name": "contact-keeper",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "knex migrate:up --env test",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "posttest": "knex migrate:down --env test",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "cross-env NODE_ENV='development' tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\"",
    "migrate:make": "knex migrate:make contact_keeper -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "prisma": "prisma",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^3.0.2",
    "@fastify/autoload": "^5.2.0",
    "@fastify/cookie": "^8.0.0",
    "@fastify/env": "^4.1.0",
    "@fastify/jwt": "^6.3.2",
    "@prisma/client": "^3.5.x",
    "altair-fastify-plugin": "^4.6.4",
    "bcrypt": "^5.0.1",
    "fastify": "^4.5.3",
    "fastify-auth": "^1.2.0",
    "fastify-autoload": "^3.13.0",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cli": "^5.3.0",
    "fastify-cookie": "^5.7.0",
    "fastify-env": "^2.2.0",
    "fastify-jwt": "4.2.0",
    "fastify-plugin": "^4.2.1",
    "fastify-redis": "^4.4.0",
    "fastify-sensible": "^3.2.0",
    "fastify-swagger": "^5.2.0",
    "graphql": "^16.6.0",
    "graphql-upload": "^16.0.1",
    "ioredis": "^5.2.3",
    "mercurius": "^10.3.0",
    "mercurius-auth": "^3.0.0",
    "mercurius-upload": "^5.0.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^18.7.13",
    "@types/tap": "^15.0.7",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "^3.5.x",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
